{
    "daemon":
    {
        "exe": ["python3", "etcd_daemon.py"],
        "retry":
        {
            "times": 3,
            "interval": 5
        },
        "pid_file": "data/etcd.pid",
        "raw_log_symbol_pos": 27,
        "log_daemon":
        {
            "info": "log/etcd/daemon_info.log",
            "error": "log/etcd/daemon_error.log"
        },
        "log_etcd":
        {
            "info": "log/etcd/etcd_info.log",
            "error": "log/etcd/etcd_error.log"
        }
    },
    "etcd":
    {
        "exe": "etcd",
        "name": "etcd",
        "data_dir": "data/etcd",
        "data_init_dir": "data/etcd_init",
        "listen":
        {
            "address": "0.0.0.0",
            "peer_port": "2000",
            "client_port": "2001"
        },
        "advertise":
        {
            "address": "localhost",
            "peer_port": "2000",
            "client_port": "2001"
        },
        /*
        "cluster":
        {
            "type": "init",
            "discovery": "https://discovery.etcd.io/token"
        },
        "cluster":
        {
            "type": "init",
            "member": "name=url"
        },
        "cluster":
        {
            "type": "join",
            "client": "url"
        },
        "cluster":
        {
            "type": "join",
            "member": "name=url"
        },
        */
        "cluster":
        {
            "type": "init"
        }
    }
}