{
    "daemon":
    {
        "retry":
        {
            "times": 3,
            "interval": 5
        },
        "maintain_file": "data/mongodb.maintain",
        "pid_file": "data/mongodb.pid",
        "etcd_path":
        {
            "primary": "mongodb/primary"
        },
        "register_interval": 5
        /*
        "log_daemon":
        {
            "info": "log/mongodb/daemon_info.log",
            "error": "log/mongodb/daemon_error.log"
        },
        "log_mongodb":
        {
            "info": "log/mongodb/mongodb_info.log",
            "error": "log/mongodb/mongodb_error.log"
        }
        */
    },
    "mongodb":
    {
        "exe": "mongod",
        "name": "mongodb1",
        "data_dir": "data/mongodb",
        "listen":
        {
            "address": "0.0.0.0",
            "port": "3000"
        },
        "advertise":
        {
            "address": "127.0.0.1",
            "port": "3000"
        },
        "replica_set": "rs"
    }
}